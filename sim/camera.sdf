<?xml version="1.0" ?>
<!--
// yaw:
gz topic -t /z_joint_position -m gz.msgs.Double -p 'data: 7.0'
gz topic -t /z_joint_position -m gz.msgs.Double -p 'data: 5.6'
// roll:
gz topic -t /x_joint_position -m gz.msgs.Double -p 'data: 5.6'
gz topic -t /x_joint_position -m gz.msgs.Double -p 'data: 7.0'
// pitch:
gz topic -t /y_joint_position -m gz.msgs.Double -p 'data: 6.67'
gz topic -t /y_joint_position -m gz.msgs.Double -p 'data: 5.9'
// translation: 
gz topic -t "/z_trans_position" -m gz.msgs.Double -p "data: 0.5"
gz topic -t "/y_trans_position" -m gz.msgs.Double -p "data: 0.5"
gz topic -t "/x_trans_position" -m gz.msgs.Double -p "data: 0.5"
-->

<sdf version="1.10">
  <world name="cam_world">
    <gravity>0 0 -9.81</gravity>
    <physics name="1ms" type="ignored">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
    </physics>
    <plugin
        filename="gz-sim-physics-system"
        name="gz::sim::systems::Physics">
    </plugin>
    <plugin
        filename="gz-sim-scene-broadcaster-system"
        name="gz::sim::systems::SceneBroadcaster">
    </plugin>
    <plugin
        filename="ignition-gazebo-sensors-system"
        name="ignition::gazebo::systems::Sensors">
      <render_engine>ogre</render_engine>
    </plugin>
    <plugin filename="gz-sim-imu-system"
            name="gz::sim::systems::Imu">
    </plugin>
    <gui fullscreen="0">
      <plugin filename="MinimalScene" name="3D View">
        <gz-gui>
          <title>3D View</title>
          <property type="bool" key="showTitleBar">false</property>
          <property type="string" key="state">docked</property>
        </gz-gui>
        <engine>ogre2</engine>
        <scene>scene</scene>
        <ambient_light>0.4 0.4 0.4</ambient_light>
        <background_color>0.8 0.8 0.8</background_color>
        <camera_pose>-6 0 6 0 0.5 0</camera_pose>
        <camera_clip>
          <near>0.25</near>
          <far>25000</far>
        </camera_clip>
      </plugin>
      <plugin filename="GzSceneManager" name="Scene Manager">
        <gz-gui>
          <property key="resizable" type="bool">false</property>
          <property key="width" type="double">5</property>
          <property key="height" type="double">5</property>
          <property key="state" type="string">floating</property>
          <property key="showTitleBar" type="bool">false</property>
        </gz-gui>
      </plugin>
      <plugin filename="WorldControl" name="World control">
        <gz-gui>
          <title>World control</title>
          <property type="bool" key="showTitleBar">false</property>
          <property type="bool" key="resizable">false</property>
          <property type="double" key="height">72</property>
          <property type="double" key="width">121</property>
          <property type="double" key="z">1</property>
          
          <property type="string" key="state">floating</property>
          <anchors target="3D View">
            <line own="left" target="left"/>
            <line own="bottom" target="bottom"/>
          </anchors>
        </gz-gui>
        
        <play_pause>true</play_pause>
        <step>true</step>
        <start_paused>true</start_paused>
        <service>/world/world_demo/control</service>
        <stats_topic>/world/world_demo/stats</stats_topic>
      </plugin>
      <plugin filename="WorldStats" name="World stats">
        <gz-gui>
          <title>World stats</title>
          <property type="bool" key="showTitleBar">false</property>
          <property type="bool" key="resizable">false</property>
          <property type="double" key="height">110</property>
          <property type="double" key="width">290</property>
          <property type="double" key="z">1</property>
          
          <property type="string" key="state">floating</property>
          <anchors target="3D View">
            <line own="right" target="right"/>
            <line own="bottom" target="bottom"/>
          </anchors>
        </gz-gui>
        
        <sim_time>true</sim_time>
        <real_time>true</real_time>
        <real_time_factor>true</real_time_factor>
        <iterations>true</iterations>
        <topic>/world/world_demo/stats</topic>
      </plugin>
      <plugin filename="InteractiveViewControl" name="Interactive view control">
        <gz-gui>
        </gz-gui>
      </plugin>
      <plugin filename="EntityTree" name="Entity tree">
      </plugin>
      <plugin filename="ImageDisplay" name="Image Display">
        <topic>camera</topic>
      </plugin>
    </gui>
    <light type="directional" name="sun">
      <cast_shadows>false</cast_shadows>
      <pose>0 0 10 0 0 0</pose>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.0 0.0 0.0 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>
    <scene>
      <ambient>1.0 1.0 1.0</ambient>
      <background>0.8 0.8 0.8</background>
      <grid>true</grid>
    </scene>
    <model name="ground_plane">
      <static>true</static>
      <link name="link">
        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
            </plane>
          </geometry>
        </collision>
        <!--
            <visual name="visual">
            <geometry>
            <plane>
            <normal>0 0 1</normal>
            <size>100 100</size>
            </plane>
            </geometry>
            <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.8 0.8 0.8 1</specular>
            </material>
            </visual>
        -->
      </link>
    </model>
    <model name='camera_model' canonical_link="base_link">
      <!--
          <plugin filename="gz-sim-pose-publisher-system"
          name="gz::sim::systems::PosePublisher">
          <publish_model_pose>true</publish_model_pose>
          <publish_link_pose>false</publish_link_pose>
          <publish_sensor_pose>false</publish_sensor_pose>
          <publish_collision_pose>false</publish_collision_pose>
          <publish_visual_pose>false</publish_visual_pose>
          <publish_nested_model_pose>false</publish_nested_model_pose>
          <static_publisher>false</static_publisher>
          </plugin>
      -->
      <plugin filename="gz-sim-odometry-publisher-system"
               name="gz::sim::systems::OdometryPublisher">
        <odom_frame>odom_world</odom_frame>
        <robot_base_frame>base_link</robot_base_frame>
        <dimensions>3</dimensions>
      </plugin>
      <!-- starting frame
	   <pose relative_to='world'>0 0 1.0 0 0 0</pose>
	     -->
      <!--
          <link name="fixed_link">
          <pose relative_to='__model__'>0 0 0.0 0 0 0</pose>
          </link>
      -->
      <link name="start_link">
	<pose>0 0 1.0 0 0 0</pose>
      </link>
      <link name="camera_link_x">
	<pose relative_to="start_link">0 0 0 0 0 0</pose>
        <inertial>
          <mass>
            1e-9
          </mass>
          <inertia>
            <ixx>1e-9</ixx>
            <iyy>1e-9</iyy>
            <izz>1e-9</izz>
          </inertia>
        </inertial>
      </link>
      <link name="camera_link_y">
	<pose relative_to="camera_link_x">0 0 0 0 0 0</pose>
        <inertial>
          <mass>
            1e-9
          </mass>
          <inertia>
            <ixx>1e-9</ixx>
            <iyy>1e-9</iyy>
            <izz>1e-9</izz>
          </inertia>
        </inertial>
      </link>
      <link name="camera_link_z">
	<pose relative_to="camera_link_y">0 0 0 0 0 0</pose>
        <inertial>
          <mass>
            1e-9
          </mass>
          <inertia>
            <ixx>1e-9</ixx>
            <iyy>1e-9</iyy>
            <izz>1e-9</izz>
          </inertia>
        </inertial>
      </link>
      <link name="camera_link_psi">
	<pose relative_to="camera_link_z">0 0 0 0 0 0</pose>
        <inertial>
          <mass>
            1e-9
          </mass>
          <inertia>
            <ixx>1e-3</ixx>
            <iyy>1e-3</iyy>
            <izz>1e-3</izz>
          </inertia>
        </inertial>
      </link>
      <link name="camera_link_theta">
	<pose relative_to="camera_link_psi">0 0 0 0 0 0</pose>
        <inertial>
          <mass>
            1e-9
          </mass>
          <inertia>
            <ixx>1e-3</ixx>
            <iyy>1e-3</iyy>
            <izz>1e-3</izz>
          </inertia>
        </inertial>
      </link>
      <link name="base_link">
        <pose relative_to='camera_link_theta'>0 0 0.0 0 0 0</pose>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.1 0.1 0.1</size>
            </box>
          </geometry>
          <material>
            <ambient>0.0 0.0 1.0 1</ambient>
            <diffuse>0.0 0.0 1.0 1</diffuse>
            <specular>0.0 0.0 1.0 1</specular>
          </material>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.1 0.1 0.1</size>
            </box>
          </geometry>
        </collision>
        <inertial>
          <mass>
            1e-1
          </mass>
          <inertia>
            <ixx>1e-3</ixx>
            <iyy>1e-3</iyy>
            <izz>1e-3</izz>
          </inertia>
        </inertial>
        <sensor name="camera" type="camera">
          <camera>
            <horizontal_fov>1.349481884447</horizontal_fov>
            <image>
              <width>800</width>
              <height>500</height>
            </image>
            <clip>
              <near>0.1</near>
              <far>100</far>
            </clip>
          </camera>
          <always_on>1</always_on>
          <update_rate>10</update_rate>
          <visualize>true</visualize>
          <topic>camera</topic>
        </sensor>

        <sensor name="imu_sensor" type="imu">
          <always_on>1</always_on>
          <update_rate>100</update_rate>
          <visualize>true</visualize>
          <topic>imu</topic>
        </sensor>
      </link>
      <joint name='fixed_joint' type='fixed'>
        <parent>world</parent>
        <child>start_link</child>
      </joint>
      <joint name='x_trans_joint' type='prismatic'>
        <parent>start_link</parent>
        <child>camera_link_x</child>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>-1.0e6</lower>
            <upper>1.0e6</upper>
          </limit>
        </axis>
      </joint>
      <joint name='y_trans_joint' type='prismatic'>
        <parent>camera_link_x</parent>
        <child>camera_link_y</child>
        <axis>
          <xyz>0 1 0</xyz>
          <limit>
            <lower>-1.0e6</lower>
            <upper>1.0e6</upper>
          </limit>
        </axis>
      </joint>
      <joint name='z_trans_joint' type='prismatic'>
        <parent>camera_link_y</parent>
        <child>camera_link_z</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.0e6</lower>
            <upper>1.0e6</upper>
          </limit>
        </axis>
      </joint>
      <joint name='z_joint' type='revolute'>
        <parent>camera_link_z</parent>
        <child>camera_link_psi</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.0e6</lower>
            <upper>1.0e6</upper>
          </limit>
        </axis>
      </joint>
      <joint name='y_joint' type='revolute'>
        <parent>camera_link_psi</parent>
        <child>camera_link_theta</child>
        <axis>
          <xyz>0 1 0</xyz>
          <limit>
            <lower>-1.0e10</lower>
            <upper>1.0e10</upper>
          </limit>
        </axis>
      </joint>
      <joint name='x_joint' type='revolute'>
        <parent>camera_link_theta</parent>
        <child>base_link</child>
        <axis>
          <xyz>1 0 0</xyz>
          <limit>
            <lower>-1.0e10</lower>
            <upper>1.0e10</upper>
          </limit>
        </axis>
      </joint>
      <!-- ------------ CONTROLLERS -------------- -->
      <plugin
          filename="gz-sim-joint-position-controller-system"
          name="gz::sim::systems::JointPositionController">
        <joint_name>x_trans_joint</joint_name>
        <topic>x_trans_position</topic>
        <p_gain>0.5</p_gain>
        <i_gain>0.0</i_gain>
        <d_gain>0.5</d_gain>
        <i_max>1e-9</i_max>
        <i_min>-1e-9</i_min>
        <cmd_max>1.0</cmd_max>
        <cmd_min>-1.0</cmd_min>
      </plugin>

      <plugin
          filename="gz-sim-joint-position-controller-system"
          name="gz::sim::systems::JointPositionController">
        <joint_name>y_trans_joint</joint_name>
        <topic>y_trans_position</topic>
        <p_gain>0.5</p_gain>
        <i_gain>0.0</i_gain>
        <d_gain>0.5</d_gain>
        <i_max>1e-9</i_max>
        <i_min>-1e-9</i_min>
        <cmd_max>1.0</cmd_max>
        <cmd_min>-1.0</cmd_min>
      </plugin>
      
      <plugin
          filename="gz-sim-joint-position-controller-system"
          name="gz::sim::systems::JointPositionController">
        <joint_name>z_trans_joint</joint_name>
        <topic>z_trans_position</topic>
        <p_gain>0.5</p_gain>
        <i_gain>0.0</i_gain>
        <d_gain>0.5</d_gain>
        <i_max>1e-9</i_max>
        <i_min>-1e-9</i_min>
        <cmd_max>3.0</cmd_max>
        <cmd_min>-3.0</cmd_min>
        <cmd_offset>0.981</cmd_offset>
      </plugin>
      <!-- rotational controllers -->
      <plugin
          filename="gz-sim-joint-position-controller-system"
          name="gz::sim::systems::JointPositionController">
        <joint_name>x_joint</joint_name>
        <topic>x_rot_position</topic>
        <p_gain>1.0</p_gain>
        <i_gain>0.0</i_gain>
        <d_gain>0.2</d_gain>
        <i_max>1</i_max>
        <i_min>-1</i_min>
        <cmd_max>2</cmd_max>
        <cmd_min>-2</cmd_min>
      </plugin>
      <plugin
          filename="gz-sim-joint-position-controller-system"
          name="gz::sim::systems::JointPositionController">
        <joint_name>y_joint</joint_name>
        <topic>y_rot_position</topic>
        <p_gain>1.0</p_gain>
        <i_gain>0.0</i_gain>
        <d_gain>0.2</d_gain>
        <i_max>1</i_max>
        <i_min>-1</i_min>
        <cmd_max>2</cmd_max>
        <cmd_min>-2</cmd_min>
      </plugin>
      <plugin
          filename="gz-sim-joint-position-controller-system"
          name="gz::sim::systems::JointPositionController">
        <joint_name>z_joint</joint_name>
        <topic>z_rot_position</topic>
        <p_gain>1.0</p_gain>
        <i_gain>0.0</i_gain>
        <d_gain>0.2</d_gain>
        <i_max>1</i_max>
        <i_min>-1</i_min>
        <cmd_max>2</cmd_max>
        <cmd_min>-2</cmd_min>
      </plugin>
      <!-- ------------ END OF CONTROLLERS -------------- -->
      <!--
          <plugin
          filename="gz-sim-joint-state-publisher-system"
          name="gz::sim::systems::JointStatePublisher">
          <joint_name>z_trans_joint</joint_name>
          <topic>z_trans_state</topic>
          </plugin>
          <plugin
          filename="gz-sim-joint-state-publisher-system"
          name="gz::sim::systems::JointStatePublisher">
          <joint_name>z_joint</joint_name>
          <topic>z_joint_state</topic>
          </plugin>
      -->
    </model>

